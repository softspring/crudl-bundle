{
    "name": "softspring/crudl-bundle",
    "description": "A CRUD+list helper bundle.",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "softspring/core-bundle": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.*",
        "doctrine/orm": "^2.5",
        "symfony/console": "^3.0 || ^4.0 || ^5.0",
        "symfony/dependency-injection": "^3.0 || ^4.0 || ^5.0",
        "symfony/form": "^3.0 || ^4.0 || ^5.0",
        "symfony/framework-bundle": "^3.0 || ^4.0 || ^5.0",
        "symfony/twig-bridge": "^3.0 || ^4.0 || ^5.0",
        "symfony/yaml": "^3.0 || ^4.0 || ^5.0",
        "twig/twig": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\CrudlBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\CrudlBundle\\Tests\\": "/Tests/"
        }
    },
    "suggest": {
        "javihgil/doctrine-pagination-bundle": "Paginate doctrine repositories easily."
    },
    "conflict": {
        "javihgil/doctrine-pagination-bundle": "1.0.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
