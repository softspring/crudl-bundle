{
    "name": "softspring/crudl-bundle",
    "description": "A CRUD+list helper bundle.",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "softspring/core-bundle": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.*",
        "doctrine/orm": "^2.5",
        "symfony/console": "^4.4 || ^5.3 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.3 || ^6.0",
        "symfony/form": "^4.4 || ^5.3 || ^6.0",
        "symfony/framework-bundle": "^4.4 || ^5.3 || ^6.0",
        "symfony/twig-bridge": "^4.4 || ^5.3 || ^6.0",
        "symfony/yaml": "^4.4 || ^5.3 || ^6.0",
        "twig/twig": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\CrudlBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\CrudlBundle\\Tests\\": "/tests/"
        }
    },
    "suggest": {
        "javihgil/doctrine-pagination-bundle": "Paginate doctrine repositories easily."
    },
    "conflict": {
        "javihgil/doctrine-pagination-bundle": "1.0.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
