{
    "name": "softspring/crudl-bundle",
    "description": "A CRUD+list helper bundle.",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "softspring/crudl-controller": "^5.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.51.*",
        "phpmd/phpmd": "dev-master",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5.5"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\CrudlBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\CrudlBundle\\Tests\\": "tests/"
        }
    }
}
